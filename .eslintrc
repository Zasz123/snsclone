{
  "extends": "airbnb",
  "env": {
    "node":true,
    "mocha":true,
    "es6": true
  },
  "rules": {
    "comma-dangle": 0,
    "max-len": ["error", {"code": 120, "ignoreComments": true, "ignoreUrls": true, "ignoreStrings": true}],
    "no-unused-vars": 0,
    "prefer-destructuring": 0,
    "no-plusplus": 0,
    "no-array-constructor": 0
  }
}
